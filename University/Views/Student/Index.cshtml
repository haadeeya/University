<!DOCTYPE html>
<html lang="de">

<head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="~/Content/index.css">
    <script src="//ajax.googleapis.com/ajax/libs/dojo/1.14.1/dojo/dojo.js" data-dojo-config="async: true"></script>
    <script src="~/Scripts/indexDojo.js" type="text/javascript"></script>
    <title>Student Profile</title>

</head>

<body>

    <div class="container">
        <h1 id="names"></h1>
        <div class="profile">

                <div class="profile-property">
                    <div class="property-name">First Name</div>
                    <div class="property-value" id="fname"></div>
                </div>

                <div class="profile-property">
                    <div class="property-name">Last Name</div>
                    <div class="property-value" id="lname"></div>
                </div>

                <div class="profile-property">
                    <div class="property-name">NID</div>
                    <div class="property-value" id="nid"></div>
                </div>

                <div class="profile-property">
                    <div class="property-name">Date Of Birth</div>
                    <div class="property-value" id="dob"></div>
                </div>

                <div class="profile-property">
                    <div class="property-name">Email Address</div>
                    <div class="property-value" id="emailaddress"></div>
                </div>

                <div class="profile-property">
                    <div class="property-name">Phone Number</div>
                    <div class="property-value" id="phonenum"></div>
                </div>

                <div class="profile-property">
                    <div class="property-name">Guardian Name</div>
                    <div class="property-value" id="guardianname"></div>
                </div>

                <div class="profile-property" id="subjects"> <span style="font-size:20px; font-weight:bold;">Results</span>
                    <div class="property-name"></div>
                    <div class="property-value"></div>
                </div>

                <div style="text-align:right">
                    <a href="/Student/Edit"><img src="https://i.pinimg.com/564x/fe/df/91/fedf910a5a393a12a90350979d4e5057.jpg" width="20" height="20" /></a>
                </div>
            <br />
                <div style="text-align:right">
                    <a href="/Account/Logout"><img src="https://cdn-icons-png.flaticon.com/512/126/126467.png" width="20" height="20" alt="Logout"/></a>
                </div>
            </div>
    </div>

</body>

</html>

